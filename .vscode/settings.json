{
    "deploy": {
        "showPopupOnSuccess": false,
        "packages": [{
            "name": "twoss-io.github.io",
            "description": "twoss-io.github.io",
            "files": [
                "**/*.js",
                "**/*.html",
                "**/*.htm",
                "**/*.css"
            ],
            "deployOnSave": true
        }],
        "targets": [{
            "type": "local",
            "name": "My local folder",
            "description": "A local folder",
            "dir": "../twoss/public"
        }]
    }
}